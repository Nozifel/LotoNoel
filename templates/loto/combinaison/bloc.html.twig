<div class=""></div>

{% for i in 0..loto.hauteurGrille-1 %}
    <div>
        {% for j in 0..loto.largeurGrille-1 %}
            <span class="checkbox-group">
                <input type="checkbox" name="c[{{ i }}][{{ j }}]" id="c[{{ i }}][{{ j }}]">
                <label for="c[{{ i }}][{{ j }}]"></label>
            </span>
        {% endfor %}
    </div>
{% endfor %}